<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/descripcion2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="83dp"
        android:layout_marginRight="83dp"
        android:layout_marginBottom="6dp"
        android:layout_weight="1"
        android:text="Descripcion"
        app:layout_constraintBottom_toTopOf="@+id/estado2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/descripcion"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/fecha2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="Fecha"
        app:layout_constraintBottom_toBottomOf="@+id/fecha"
        app:layout_constraintEnd_toEndOf="@+id/estado2"
        app:layout_constraintStart_toStartOf="@+id/estado2"
        app:layout_constraintTop_toTopOf="@+id/fecha"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/nombre2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="Nombre"
        app:layout_constraintBottom_toBottomOf="@+id/Nombre"
        app:layout_constraintEnd_toEndOf="@+id/fecha2"
        app:layout_constraintStart_toStartOf="@+id/fecha2"
        app:layout_constraintTop_toTopOf="@+id/Nombre" />

    <TextView
        android:id="@+id/telefono2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="Telefono"
        app:layout_constraintBottom_toBottomOf="@+id/Telefono"
        app:layout_constraintEnd_toEndOf="@+id/nombre2"
        app:layout_constraintStart_toStartOf="@+id/nombre2"
        app:layout_constraintTop_toTopOf="@+id/Telefono" />

    <TextView
        android:id="@+id/estado2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="Estado"
        app:layout_constraintBottom_toBottomOf="@+id/estado"
        app:layout_constraintEnd_toEndOf="@+id/descripcion2"
        app:layout_constraintStart_toStartOf="@+id/descripcion2"
        app:layout_constraintTop_toTopOf="@+id/estado" />

    <TextView
        android:id="@+id/descripcion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="97dp"
        android:layout_marginRight="97dp"
        android:layout_weight="1"
        android:text="Descripcion"
        app:layout_constraintEnd_toStartOf="@+id/descripcion2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/fecha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="11dp"
        android:layout_weight="1"
        android:text="Fecha"
        app:layout_constraintBottom_toTopOf="@+id/Nombre"
        app:layout_constraintEnd_toEndOf="@+id/estado"
        app:layout_constraintStart_toStartOf="@+id/estado" />

    <TextView
        android:id="@+id/Nombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="7dp"
        android:layout_weight="1"
        android:text="Nombre"
        app:layout_constraintBottom_toTopOf="@+id/Telefono"
        app:layout_constraintEnd_toEndOf="@+id/fecha"
        app:layout_constraintStart_toStartOf="@+id/fecha" />

    <TextView
        android:id="@+id/Telefono"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="166dp"
        android:layout_weight="1"
        android:text="Telefono"
        app:layout_constraintEnd_toEndOf="@+id/Nombre"
        app:layout_constraintStart_toStartOf="@+id/Nombre"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/estado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="13dp"
        android:layout_weight="1"
        android:text="Estado"
        app:layout_constraintBottom_toTopOf="@+id/fecha"
        app:layout_constraintEnd_toEndOf="@+id/descripcion"
        app:layout_constraintStart_toStartOf="@+id/descripcion" />


</android.support.constraint.ConstraintLayout>